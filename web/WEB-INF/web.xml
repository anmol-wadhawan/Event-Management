<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>NewServlet111</servlet-name>
        <servlet-class>controller.NewServlet111</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet123</servlet-name>
        <servlet-class>controller.NewServlet123</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DemoServlett</servlet-name>
        <servlet-class>controller.DemoServlett</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>regservlet</servlet-name>
        <servlet-class>controller.regservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet111</servlet-name>
        <url-pattern>/NewServlet111</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet123</servlet-name>
        <url-pattern>/NewServlet123</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DemoServlett</servlet-name>
        <url-pattern>/DemoServlett</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>regservlet</servlet-name>
        <url-pattern>/regservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
